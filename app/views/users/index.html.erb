<h1>Users#index</h1>
<table>
	<tr>
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td> <%= link_to user.name, user_path(user.id)%></td>
			<td> <%= user.email %></td>
			<td> <%= user.password %></td>

		</tr>
	<% end %>
</table>

<%= link_to "Add new user", new_user_path%>











